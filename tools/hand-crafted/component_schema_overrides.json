[
  {"_comment": "--- embed_nbt: extractor can't detect CompoundTag-based codecs ---"},
  {"name": "minecraft:bucket_entity_data", "pattern": "embed_nbt"},
  {"name": "minecraft:custom_data", "pattern": "embed_nbt"},
  {"name": "minecraft:map_decorations", "pattern": "embed_nbt"},

  {"_comment": "--- delegate: go-mc API choice for named field wrappers ---"},
  {"name": "minecraft:break_sound", "pattern": "delegate", "fieldName": "Sound", "fieldType": "SoundEvent", "serMethod": "direct"},
  {"name": "minecraft:container_loot", "pattern": "delegate", "fieldName": "Data", "fieldType": "dynbt.Value", "serMethod": "nbtfield"},
  {"name": "minecraft:custom_name", "pattern": "delegate", "fieldName": "Name", "fieldType": "chat.Message", "serMethod": "direct"},
  {"name": "minecraft:debug_stick_state", "pattern": "delegate", "fieldName": "Data", "fieldType": "block.State", "serMethod": "nbt"},
  {"name": "minecraft:enchantment_glint_override", "pattern": "delegate", "fieldName": "HasGlint", "fieldType": "pk.Boolean", "serMethod": "direct"},
  {"name": "minecraft:item_name", "pattern": "delegate", "fieldName": "Name", "fieldType": "chat.Message", "serMethod": "direct"},
  {"name": "minecraft:lock", "pattern": "delegate", "fieldName": "Data", "fieldType": "dynbt.Value", "serMethod": "nbtfield"},
  {"name": "minecraft:recipes", "pattern": "delegate", "fieldName": "Data", "fieldType": "dynbt.Value", "serMethod": "nbt"},
  {"name": "minecraft:repairable", "pattern": "delegate", "fieldName": "Items", "fieldType": "IDSet", "serMethod": "direct"},
  {"name": "minecraft:use_remainder", "pattern": "delegate", "fieldName": "Remainder", "fieldType": "SlotData", "serMethod": "direct"},

  {"_comment": "--- named_int: enum types with Go constants ---"},
  {"name": "minecraft:map_post_processing", "pattern": "named_int", "baseType": "int32"},
  {"name": "minecraft:rarity", "pattern": "named_int", "baseType": "int32"},

  {"_comment": "--- embed: extractor produces tuple/different, existing embed is preferred ---"},
  {"name": "minecraft:firework_explosion", "pattern": "embed", "embedType": "ItemFireworkExplosion"},

  {"_comment": "--- array: extractor can't detect or uses wrong names/types ---"},
  {"name": "minecraft:banner_patterns", "pattern": "array", "fieldName": "Layers", "elementType": "BannerPatternLayer"},
  {"name": "minecraft:bees", "pattern": "array", "fieldName": "Bees", "elementType": "BeeData"},
  {"name": "minecraft:block_state", "pattern": "array", "fieldName": "Properties", "elementType": "BlockStateProperty"},
  {"name": "minecraft:bundle_contents", "pattern": "array", "fieldName": "Contents", "elementType": "SlotData"},
  {"name": "minecraft:can_break", "pattern": "array", "fieldName": "Predicates", "elementType": "ItemBlockPredicate"},
  {"name": "minecraft:can_place_on", "pattern": "array", "fieldName": "Predicates", "elementType": "ItemBlockPredicate"},
  {"name": "minecraft:charged_projectiles", "pattern": "array", "fieldName": "Projectiles", "elementType": "SlotData"},
  {"name": "minecraft:container", "pattern": "array", "fieldName": "Contents", "elementType": "SlotData"},
  {"name": "minecraft:death_protection", "pattern": "array", "fieldName": "Effects", "elementType": "ItemConsumeEffect"},
  {"name": "minecraft:enchantments", "pattern": "array", "fieldName": "Enchantments", "elementType": "EnchantmentEntry"},
  {"name": "minecraft:lore", "pattern": "array", "fieldName": "Lines", "elementType": "chat.Message"},
  {"name": "minecraft:pot_decorations", "pattern": "array", "fieldName": "Decorations", "elementType": "pk.VarInt"},
  {"name": "minecraft:stored_enchantments", "pattern": "array", "fieldName": "Enchantments", "elementType": "EnchantmentEntry"},
  {"name": "minecraft:suspicious_stew_effects", "pattern": "array", "fieldName": "Effects", "elementType": "StewEffect"},
  {"name": "minecraft:writable_book_content", "pattern": "array", "fieldName": "Pages", "elementType": "Page"},

  {"_comment": "--- tuple: field name/type corrections for Go-idiomatic naming ---"},
  {"name": "minecraft:block_entity_data", "pattern": "tuple", "fields": [
    {"name": "Type", "type": "pk.VarInt"},
    {"name": "Data", "type": "pk.NBTField[dynbt.Value]"}
  ]},
  {"name": "minecraft:consumable", "pattern": "tuple", "fields": [
    {"name": "ConsumeSeconds", "type": "pk.Float"},
    {"name": "Animation", "type": "pk.VarInt"},
    {"name": "Sound", "type": "SoundEvent"},
    {"name": "MakesParticles", "type": "pk.Boolean"},
    {"name": "Effects", "type": "pk.Array[ItemConsumeEffect]"}
  ]},
  {"name": "minecraft:custom_model_data", "pattern": "tuple", "fields": [
    {"name": "Floats", "type": "pk.Array[pk.Float]"},
    {"name": "Flags", "type": "pk.Array[pk.Boolean]"},
    {"name": "Strings", "type": "pk.Array[pk.String]"},
    {"name": "Colors", "type": "pk.Array[pk.Int]"}
  ]},
  {"name": "minecraft:entity_data", "pattern": "tuple", "fields": [
    {"name": "Type", "type": "pk.VarInt"},
    {"name": "Data", "type": "pk.NBTField[dynbt.Value]"}
  ]},
  {"name": "minecraft:equippable", "pattern": "tuple", "fields": [
    {"name": "Slot", "type": "pk.VarInt"},
    {"name": "Sound", "type": "SoundEvent"},
    {"name": "Model", "type": "pk.Option[pk.String]"},
    {"name": "CameraOverlay", "type": "pk.Option[pk.String]"},
    {"name": "AllowedEntities", "type": "pk.Option[IDSet]"},
    {"name": "Dispensable", "type": "pk.Boolean"},
    {"name": "Swappable", "type": "pk.Boolean"},
    {"name": "Damageable", "type": "pk.Boolean"},
    {"name": "EquipOnInteract", "type": "pk.Boolean"},
    {"name": "Shearable", "type": "pk.Boolean"},
    {"name": "ShearingSound", "type": "SoundEvent"}
  ]},
  {"name": "minecraft:fireworks", "pattern": "tuple", "fields": [
    {"name": "FlightDuration", "type": "pk.VarInt"},
    {"name": "Explosions", "type": "pk.Array[ItemFireworkExplosion]"}
  ]},
  {"name": "minecraft:kinetic_weapon", "pattern": "tuple", "fields": [
    {"name": "ContactCooldownTicks", "type": "pk.VarInt"},
    {"name": "DelayTicks", "type": "pk.VarInt"},
    {"name": "DismountConditions", "type": "pk.Option[KineticWeaponCondition]"},
    {"name": "KnockbackConditions", "type": "pk.Option[KineticWeaponCondition]"},
    {"name": "DamageConditions", "type": "pk.Option[KineticWeaponCondition]"},
    {"name": "ForwardMovement", "type": "pk.Float"},
    {"name": "DamageMultiplier", "type": "pk.Float"},
    {"name": "Sound", "type": "pk.Option[SoundEvent]"},
    {"name": "HitSound", "type": "pk.Option[SoundEvent]"}
  ]},
  {"name": "minecraft:lodestone_tracker", "pattern": "tuple", "fields": [
    {"name": "GlobalPosition", "type": "pk.Option[GlobalPos]"},
    {"name": "Tracked", "type": "pk.Boolean"}
  ]},
  {"name": "minecraft:potion_contents", "pattern": "tuple", "fields": [
    {"name": "PotionID", "type": "pk.Option[pk.VarInt]"},
    {"name": "CustomColor", "type": "pk.Option[pk.Int]"},
    {"name": "CustomEffects", "type": "pk.Array[ItemPotionEffect]"},
    {"name": "CustomName", "type": "pk.Option[pk.String]"}
  ]},
  {"name": "minecraft:tool", "pattern": "tuple", "fields": [
    {"name": "Rules", "type": "pk.Array[ToolRule]"},
    {"name": "DefaultMiningSpeed", "type": "pk.Float"},
    {"name": "DamagePerBlock", "type": "pk.VarInt"},
    {"name": "CanDestroyBlocksInCreative", "type": "pk.Boolean"}
  ]},
  {"name": "minecraft:tooltip_display", "pattern": "tuple", "fields": [
    {"name": "HideTooltip", "type": "pk.Boolean"},
    {"name": "HiddenComponents", "type": "pk.Array[pk.VarInt]"}
  ]},
  {"name": "minecraft:written_book_content", "pattern": "tuple", "fields": [
    {"name": "RawTitle", "type": "pk.String"},
    {"name": "FilteredTitle", "type": "pk.Option[pk.String]"},
    {"name": "Author", "type": "pk.String"},
    {"name": "Generation", "type": "pk.VarInt"},
    {"name": "Pages", "type": "pk.Array[WrittenPage]"},
    {"name": "Resolved", "type": "pk.Boolean"}
  ]},

  {"_comment": "--- custom: hand-written Go implementations ---"},
  {"name": "minecraft:attribute_modifiers", "pattern": "custom"},

  {"_comment": "--- either_holder_data: EitherHolder<T> with inline data ---"},
  {"name": "minecraft:instrument", "pattern": "either_holder_data",
    "inlineType": "InstrumentData", "inlineFields": [
      {"name": "SoundEvent", "type": "SoundEvent"},
      {"name": "UseDuration", "type": "pk.Float"},
      {"name": "Range", "type": "pk.Float"},
      {"name": "Description", "type": "chat.Message"}
  ]},
  {"name": "minecraft:jukebox_playable", "pattern": "either_holder_data",
    "inlineType": "JukeboxSongData", "inlineFields": [
      {"name": "SoundEvent", "type": "SoundEvent"},
      {"name": "Description", "type": "chat.Message"},
      {"name": "LengthInSeconds", "type": "pk.Float"},
      {"name": "ComparatorOutput", "type": "pk.VarInt"}
  ]},
  {"name": "minecraft:provides_trim_material", "pattern": "either_holder_data",
    "inlineType": "ArmorTrimMaterial"},

  {"_comment": "--- holder_data: registryEntryHolder<T> with inline data ---"},
  {"name": "minecraft:painting/variant", "pattern": "holder_data",
    "inlineType": "PaintingVariantData", "inlineFields": [
      {"name": "Width", "type": "pk.VarInt"},
      {"name": "Height", "type": "pk.VarInt"},
      {"name": "AssetID", "type": "pk.String"},
      {"name": "Title", "type": "pk.Option[chat.Message]"},
      {"name": "Author", "type": "pk.Option[chat.Message]"}
  ]},

  {"_comment": "--- composite_holder: multiple registryEntryHolder fields ---"},
  {"name": "minecraft:trim", "pattern": "composite_holder", "holders": [
    {"typeName": "MaterialType", "dataName": "MaterialData", "inlineType": "ArmorTrimMaterial"},
    {"typeName": "PatternType", "dataName": "PatternData", "inlineType": "ArmorTrimPattern"}
  ]}
]
